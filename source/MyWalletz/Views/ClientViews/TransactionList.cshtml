<div id="transaction-list-page" class="page">
  <h2 class="page-header"></h2>
  <div class="data-grid">
    <table class="table table-striped table-bordered">
      <thead>
        <tr class="data-grid-header">
          <th data-attribute="postedat">Posted at</th>
          <th data-attribute="category">Category</th>
          <th data-attribute="payee">Payee</th>
          <th data-attribute="amount">Amount</th>
        </tr>
      </thead>
      <tfoot>
        <tr>
          <th colspan="4" id="transaction-list-balance"></th>
        </tr>
      </tfoot>
      <tbody class="data-grid-content"></tbody>
    </table>
    <div class="pagination pagination-right"></div>
  </div>
  <div class="form-actions">
    <a class="btn btn-primary" href="javascript:;">
      <i class="icon-plus icon-white"></i> Create new
    </a>
  </div>
  <script id="transaction-grid-row-template" type="text/html">
    <td>{{formattedPostedAt()}}</td>
    <td>{{categoryTitle}}</td>
    <td>{{payee}}</td>
    <td>{{formattedAmount()}}</td>
  </script>
</div>